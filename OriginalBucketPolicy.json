{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowLambdaReadAccessByCondition",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::riley-original-images-bucket/*",
      "Condition": {
        "StringEquals": {
          "aws:PrincipalArn": "arn:aws:iam::445567091924:role/service-role/ResizeImageFunction-role-33uqskhs"
        }
      }
    },
    {
      "Sid": "AllowPublicReadAccessToOriginalImages",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::riley-original-images-bucket/*"
    }
  ]
}